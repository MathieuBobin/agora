<% if params[:city_id]  %>
  <% unless @ongoing_proposals_count == 0%>
    <% if (@proposal.votes_count < 5) && (@proposal.differencebetween == false) %>
      proposalId = <%= @proposal.id %>;
      proposalRow = $('tr#'+proposalId);
      voteInfo = proposalRow.find($('.vote-info'));
      voteInfo.remove();
      proposalRow.append('<%= j render 'cities/vote_informations', x: @proposal %>');
    <% else %>
      window.location = '<%= city_path @city %>'
    <% end %>
  <% else %>
    window.location = '<%= city_path @city %>'
  <% end %>
<% else %>
  proposalInfo = $('#proposal-info');
  voteInfo = proposalInfo.find($('#proposal-vote-info'));
  voteInfo.remove();
  proposalInfo.append('<%= j render 'proposals/proposal_vote_informations', proposal: @proposal %>');
<% end %>